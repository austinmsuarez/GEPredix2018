#!/usr/bin/expect

    set reponame [lindex $argv 0];
    set username [lindex $argv 1];
    set password [lindex $argv 2];
    
    set timeout 20
    spawn library/scripts/npmLoginAndPublish.sh $reponame $username $password
    
    expect "*sername*"
    send "$username\r"

    expect "*assword*"
    send "$password\r"

    expect "Email: (this IS public)*"
    send "predix.adoption@ge.com\r"

    expect "Logged in as predix*"

    expect "*+ $reponame@*"
